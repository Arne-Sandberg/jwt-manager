{
  "name": "jwt-manager",
  "version": "1.1.9",
  "description": "A JS Library for managing JWT Tokens",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "production": "webpack -p",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeorgeHanson/jwt-manager.git"
  },
  "keywords": [
    "Javascript",
    "JWT",
    "JWT",
    "Token",
    "Authentication"
  ],
  "author": "George Hanson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GeorgeHanson/jwt-manager/issues"
  },
  "homepage": "https://github.com/GeorgeHanson/jwt-manager#readme",
  "devDependencies": {
    "jest": "^20.0.4",
    "jest-localstorage-mock": "^1.2.0",
    "mock-cookie": "^0.3.2",
    "regenerator-runtime": "^0.10.5",
    "ts-jest": "^20.0.7",
    "babel": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-es2016": "^6.16.0",
    "ts-loader": "^2.0.1",
    "typescript": "^2.1.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-2": "^6.5.0",
    "rollup": "^0.31.0",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-typescript": "^0.7.5"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
